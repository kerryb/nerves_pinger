<table class="table table-striped">
  <thead>
    <th>Type</th>
    <th>Address</th>
    <th>Status</th>
    <th>Time (ms)</th>
  </thead>
  <tbody id="results-table">
    <%= for {check, {status, time}} <- @results do %>
      <tr id="#{check.type}-#{check.address}">
        <td><%= check.type %></td>
        <td><%= check.address %></td>
        <td><%= status %></td>
        <td><%= if status == :ok, do: trunc(time) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
